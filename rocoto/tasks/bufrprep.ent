<task name="bufrprep_HAFS" maxtries="99">

      <command>bash ${JOBSdir}/JHAFS_BUFRPREP</command>
      <jobname>hafs_bufrprep</jobname>
      <queue>&QUEUE;</queue>
      <account>&ACCOUNT;</account>
      <cores>1</cores>
      <walltime>00:59:00</walltime>
      <memory>2GB</memory>

      &SERIAL_EXTRA;

      <envar>
        <name>CYCLE</name>
	<value><cyclestr>@Y@m@d@H</cyclestr></value>
      </envar>
      <envar>
         <name>PREREQSscript</name>
         <value>&PRE;</value>
      </envar>

      <dependency>
          <taskdep task="launch_HAFS"/>    
      </dependency>

</task>
