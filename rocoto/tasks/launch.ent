<task name="launch_FV3SAR_HAFS" maxtries="99">

      <command>bash &PRE;; bash ${JOBSdir}/JFV3SAR_LAUNCHER</command>
      <jobname>fv3sar_hafs_launch</jobname>
      <queue>&QUEUE;</queue>
